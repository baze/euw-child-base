{
  "name": "WPStarterTheme",
  "version": "0.1.0",
  "description": "WP starter Theme with common tasks and scenarios",
  "repository": {},
  "main": "./dest/js/main.js",
  "browser": {
    "modernizr": "./bower_components/modernizr/modernizr.js",
    "respond": "./bower_components/respond/dest/respond.min.js",
    "svgmagic": "./bower_components/SVGMagic/src/client/jquery.svgmagic.js",
    "responsiveNav": "./bower_components/responsive-nav/responsive-nav.min.js",
    "headroom": "./bower_components/headroom.js/dist/headroom.min.js",
    "jquery.flexslider": "./bower_components/flexslider/jquery.flexslider-min.js",
    "mediaelement": "./bower_components/mediaelement/build/mediaelement-and-player.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "modernizr": {
      "exports": "Modernizr"
    },
    "svgmagic": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "responsiveNav": {
      "exports": "responsiveNav"
    },
    "jquery.flexslider": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "headroom": {
      "exports": "Headroom"
    },
    "mediaelement": {
      "exports": "MediaElementPlayer",
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "^2.7.13",
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.9",
    "deamdify": "^0.1.1",
    "debowerify": "^1.3.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-changed": "^1.2.1",
    "gulp-gzip": "1.2.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.3.0",
    "gulp-insert": "^0.5.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-ng-annotate": "^1.0.0",
    "gulp-ng-autobootstrap": "^0.2.0",
    "gulp-ngmin": "^0.3.0",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-ruby-sass": "^1.0.5",
    "gulp-streamify": "1.0.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "pretty-hrtime": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.3"
  },
  "dependencies": {
    "jquery": "^2.1.4",
    "malihu-custom-scrollbar-plugin": "^3.0.9"
  }
}
