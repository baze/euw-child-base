{% block content %}
    {% if video %}
        <div class="_module-video">
            <div class="video-wrapper">
                <video id="my-video" class="video-js"
                        {{ video.attributes.autoplay ? 'autoplay' : '' }}
                        {{ video.attributes.controls ? 'controls' : '' }}
                        {{ video.attributes.loop ? 'loop' : '' }}
                       preload="{{ video.attributes.preload }}"
                       data-setup="{}">
                    <source src="{{ video.url }}" type='video/mp4'>
                </video>
            </div>
        </div>
    {% endif %}
{% endblock %}